							<addrLine rend="align(center)">(<choice>
									<abbr>Adr. Hrn.</abbr>
									<expan>Adresse Herrn</expan>
								</choice>
								<persName ref="#p0249" type="bi">Varnhagen von Ense</persName>)</addrLine>
							<addrLine rend="align(right)"><choice>
